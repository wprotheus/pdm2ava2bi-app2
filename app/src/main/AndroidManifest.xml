<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <queries>
        <provider android:authorities="com.wprotheus.wellingtonbpneto" />
    </queries>

    <uses-permission android:name="com.wprotheus.wellingtonbpneto.PERMISSION_READ" />
    <uses-permission android:name="com.wprotheus.wellingtonbpneto.PERMISSION_WRITE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/olympus"
        android:label="@string/app_name"
        android:roundIcon="@drawable/olympus"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wellingtonbpneto_ranking"
        tools:targetApi="31">
        <activity
            android:name=".view.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>